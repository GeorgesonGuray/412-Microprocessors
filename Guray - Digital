int PIN = 8; 


void setup() {
  pinMode(PIN, OUTPUT);
  Serial.begin(9600);
}


void loop() {
  digitalWrite(PIN, HIGH);
  delay(500);
  Serial.println("ON");
  
  digitalWrite(PIN, LOW);
  delay(500); 
  Serial.println("OFF");
}
